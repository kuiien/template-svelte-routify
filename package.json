{
  "private": true,
  "type": "module",
  "name": "@monkgooi/template-svelte-routify",
  "description": "Starter template for building application with Svelte, Routify, TailwindCSS with Flowbite",
  "version": "0.1.0",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --fix",
    "build": "npm run build:clean && routify -b && vite build",
    "build:clean": "rimraf dist .routify",
    "preview:dev": "npm run build && vite dev",
    "preview:dist": "npm run build && vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:monkgooi/template-svelte-routify.git"
  },
  "bugs": {
    "url": "https://github.com/monkgooi/template-svelte-routify/issues"
  },
  "author": "MonkGooi",
  "license": "MIT",
  "devDependencies": {
    "@roxi/routify": "^2.18.8",
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "flowbite-svelte": "^0.27.14",
    "html-minifier": "^4.0.0",
    "postcss": "^8.4.18",
    "postcss-load-config": "^4.0.1",
    "rimraf": "^3.0.2",
    "svelte": "^3.52.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.2.1",
    "vite": "^3.2.2"
  }
}
